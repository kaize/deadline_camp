= simple_form_for @member, :url => admin_member_path(@member), :html => {:class => "form-horizontal"} do |f|
  = f.error_notification

  = f.input :state_event, :as => :state_event
  = f.input :first_name
  = f.input :last_name
  = f.input :patronymic
  = f.input :email

  .form-actions
    = f.button :submit, :class => 'btn btn-primary'

